{
  "name": "ember-data-table",
  "version": "2.0.1",
  "description": "Data tables for Ember following Google Design specs",
  "keywords": [
    "ember-addon",
    "mu-semtech"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mu-semtech/ember-data-table"
  },
  "license": "MIT",
  "author": "Erika Pauwels",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
