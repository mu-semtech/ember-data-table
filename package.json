    "ember-resolver": "^8.0.2",
    "ember-source": "~3.21.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.11.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.8.0",
    "eslint-plugin-ember": "^8.13.0",
    "eslint-plugin-node": "^11.1.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "phantomjs": "^2.1.7",
    "qunit-dom": "^1.4.0"
