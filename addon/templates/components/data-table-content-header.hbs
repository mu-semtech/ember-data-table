<tr>
  {{#if this.enableSelection}}
    <th>{{!-- Checkbox --}}</th>
  {{/if}}
  {{#if this.enableLineNumbers}}
    <th>{{!-- Linenumbers --}}</th>
  {{/if}}
  {{#if (has-block)}}
    {{yield}}
  {{else}}
    {{#each this.fields as |field|}}
      {{th-sortable field=field label=field currentSorting=this.sort}}
    {{/each}}
  {{/if}}
</tr>
